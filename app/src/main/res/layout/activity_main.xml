<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/a2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/b2"
        app:srcCompat="#FFC107"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/g2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/h2"
        app:srcCompat="#FFC107"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/c2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/d2"
        app:srcCompat="#FFC107"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/f2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/g2"
        app:srcCompat="@android:color/holo_orange_dark"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/d2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/e2"
        app:srcCompat="@android:color/holo_orange_dark"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/h2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginEnd="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:color/holo_orange_dark"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/e2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/f2"
        app:srcCompat="#FFC107"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/b2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/c2"
        app:srcCompat="@android:color/holo_orange_dark"
        tools:layout_editor_absoluteY="474dp" />

    <ImageView
        android:id="@+id/g1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/h1"
        app:srcCompat="#FFC107"
        tools:layout_editor_absoluteY="509dp" />

    <ImageView
        android:id="@+id/f1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/g1"
        app:srcCompat="@android:color/holo_orange_dark"
        tools:layout_editor_absoluteY="509dp" />

    <ImageView
        android:id="@+id/e1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/f1"
        app:srcCompat="#FFC107"
        tools:layout_editor_absoluteY="509dp" />

    <ImageView
        android:id="@+id/a1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/b1"
        app:srcCompat="#FFC107"
        tools:layout_editor_absoluteY="509dp" />

    <ImageView
        android:id="@+id/d1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/e1"
        app:srcCompat="@android:color/holo_orange_dark"
        tools:layout_editor_absoluteY="509dp" />

    <ImageView
        android:id="@+id/b1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/c1"
        app:srcCompat="@android:color/holo_orange_dark"
        tools:layout_editor_absoluteY="509dp" />

    <ImageView
        android:id="@+id/h1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginEnd="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:color/holo_orange_dark"
        tools:layout_editor_absoluteY="509dp" />

    <ImageView
        android:id="@+id/c1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintEnd_toStartOf="@+id/d1"
        app:srcCompat="#FFC107"
        tools:layout_editor_absoluteY="509dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="130dp"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="16dp"
        android:text="@string/undo_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>